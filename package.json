{
  "name": "chatbot-farense",
  "version": "2.0.0",
  "description": "Mirobaldo - Chatbot inteligente sobre a história do Sporting Clube Farense",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "node scripts/build.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.js' --collectCoverageFrom='!src/__tests__/**'",
    "lint": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.js' 'public/**/*.js'"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "marked": "^16.4.1",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": "18.x"
  },
  "author": "Fátima Nuno",
  "license": "ISC"
}
